  <button type=\"submit\">Login</button>\n      </form>\n      {errorMessage && <p className=\"error-message\">{errorMessage}</p>}\n\n      <p>Don't have an account yet?</p>\n      <Link to={\"/signup\"}> Sign Up</Link>\n    </div>\n  );\n}\n\nexport default LoginPage;\n","/Users/alexandra.strang/code/house_helper_fullstack/home-helper-frontend/src/components/IsAnon.js",[],[],"/Users/alexandra.strang/code/house_helper_fullstack/home-helper-frontend/src/services/auth.service.js",[],[],{"ruleId":"56","severity":1,"message":"57","line":4,"column":8,"nodeType":"58","messageId":"59","endLine":4,"endColumn":13},{"ruleId":"56","severity":1,"message":"60","line":7,"column":7,"nodeType":"58","messageId":"59","endLine":7,"endColumn":14},{"ruleId":"56","severity":1,"message":"57","line":5,"column":8,"nodeType":"58","messageId":"59","endLine":5,"endColumn":13},{"ruleId":"56","severity":1,"message":"60","line":8,"column":7,"nodeType":"58","messageId":"59","endLine":8,"endColumn":14},{"ruleId":"56","severity":1,"message":"57","line":2,"column":8,"nodeType":"58","messageId":"59","endLine":2,"endColumn":13},{"ruleId":"56","severity":1,"message":"60","line":7,"column":7,"nodeType":"58","messageId":"59","endLine":7,"endColumn":14},"no-unused-vars","'axios' is defined but never used.","Identifier","unusedVar","'API_URL' is assigned a value but never used."]